{
  "kind": "collectionType",
  "collectionName": "wallets",
  "info": {
    "name": "Wallet"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "balance": {
      "type": "decimal"
    },
    "datetime": {
      "type": "datetime"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "wallet"
    }
  }
}
